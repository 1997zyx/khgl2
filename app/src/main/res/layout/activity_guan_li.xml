<?xml version="1.0" encoding="utf-8"?>
<androidx.drawerlayout.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/view"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background_bg"
    tools:context=".ui.activity.GuanLiActivity">
    <RelativeLayout

        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        >

        <RelativeLayout
            android:id="@+id/ll_title"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/colorWhite"
            android:padding="8dp">

            <com.yhkhgl.top.utils.sunUi.circularImage.CircularImage
                android:id="@+id/iv_avatar_two"
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:src="@mipmap/img_one">

            </com.yhkhgl.top.utils.sunUi.circularImage.CircularImage>

            <RelativeLayout
                android:id="@+id/rl_seach"
                android:padding="4.5dp"
                android:layout_marginRight="20dp"
                android:layout_toLeftOf="@+id/add_iv"
                android:layout_width="40dp"
                android:layout_height="40dp">
                <LinearLayout

                    android:gravity="center"
                    android:background="@drawable/yuan_bg"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <ImageView

                        android:src="@mipmap/list_icon_soushuo"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"></ImageView>
                </LinearLayout>
            </RelativeLayout>


            <ImageView
                android:id="@+id/add_iv"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_alignParentRight="true"
                android:padding="5dp"
                android:src="@mipmap/list_but_addone"></ImageView>
        </RelativeLayout>

        <LinearLayout
            android:id="@+id/title"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_below="@+id/ll_title"
            android:background="@color/colorWhite"
            android:gravity="center"
            android:orientation="vertical"
            android:paddingLeft="14dp"
            android:paddingRight="14dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp">

                <LinearLayout
                    android:id="@+id/dengji_ll"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginRight="5.5dp"
                    android:layout_weight="1"
                    android:background="@drawable/search"
                    android:gravity="center"
                    android:paddingTop="5dp"
                    android:paddingBottom="5dp">

                    <TextView
                        android:id="@+id/textView1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:text="客户等级"
                        android:textColor="@color/text_1b1b1b"
                        android:textSize="12sp"></TextView>

                    <ImageView
                        android:id="@+id/iv1"
                        android:layout_width="8dp"
                        android:layout_height="8dp"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="6dp"

                        android:src="@mipmap/list_but_more_under"></ImageView>
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_zhuangtai"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginRight="5.5dp"
                    android:layout_weight="1"
                    android:background="@drawable/search"
                    android:gravity="center"
                    android:paddingTop="5dp"
                    android:paddingBottom="5dp">

                    <TextView
                        android:id="@+id/textView2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:text="跟进状态"
                        android:textColor="@color/text_1b1b1b"
                        android:textSize="12sp"></TextView>

                    <ImageView
                        android:id="@+id/iv2"
                        android:layout_width="8dp"
                        android:layout_height="8dp"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="6dp"
                        android:src="@mipmap/list_but_more_under"></ImageView>
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_leixing"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginRight="5.5dp"
                    android:layout_weight="1"
                    android:background="@drawable/search"
                    android:gravity="center"
                    android:paddingTop="5dp"
                    android:paddingBottom="5dp">

                    <TextView
                        android:id="@+id/textView3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:text="业务类型"
                        android:textColor="@color/text_1b1b1b"
                        android:textSize="12sp"></TextView>

                    <ImageView
                        android:id="@+id/iv3"
                        android:layout_width="8dp"
                        android:layout_height="8dp"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="6dp"
                        android:src="@mipmap/list_but_more_under"></ImageView>
                </LinearLayout>

                <View
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="#FFF3F4F8"></View>

                <RelativeLayout
                    android:id="@+id/btn_saixuan"
                    android:layout_width="60dp"
                    android:layout_height="wrap_content"


                    android:paddingTop="5dp"

                    android:paddingBottom="5dp">

                    <ImageView
                        android:layout_width="12dp"
                        android:layout_height="12dp"
                        android:layout_centerVertical="true"
                        android:layout_gravity="center_vertical"
                        android:layout_marginRight="5dp"
                        android:layout_toLeftOf="@+id/tv_saixuan"
                        android:src="@mipmap/list_icon_saixuan_one"></ImageView>

                    <TextView
                        android:id="@+id/tv_saixuan"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_gravity="center_vertical"
                        android:text="筛选"
                        android:textColor="@color/text_1b1b1b"
                        android:textSize="12sp"></TextView>
                </RelativeLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <View
                    android:id="@+id/view_one"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginRight="5.5dp"
                    android:layout_weight="1"
                    android:background="#FFF3F4F8"
                    android:visibility="invisible"></View>

                <View
                    android:id="@+id/view_two"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginRight="5.5dp"
                    android:layout_weight="1"
                    android:background="#FFF3F4F8"
                    android:visibility="invisible"></View>

                <View
                    android:id="@+id/view_three"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginRight="6.5dp"
                    android:layout_weight="1"
                    android:background="#FFF3F4F8"
                    android:visibility="invisible"></View>

                <View
                    android:id="@+id/view_four"
                    android:layout_width="60dp"
                    android:layout_height="match_parent"></View>
            </LinearLayout>
        </LinearLayout>

        <RelativeLayout
            android:id="@+id/ll_content"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/title"
            android:orientation="vertical">

            <com.scwang.smartrefresh.layout.SmartRefreshLayout

                android:id="@+id/smartrefresh"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/recycleview"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="72.3dp"
                    android:paddingLeft="14dp"
                    android:paddingRight="14dp">

                </androidx.recyclerview.widget.RecyclerView>
            </com.scwang.smartrefresh.layout.SmartRefreshLayout>


            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="72.3dp"
                android:layout_alignParentBottom="true"
                android:layout_marginLeft="7dp"
                android:layout_marginRight="7dp"
                android:background="@mipmap/detail_bg_one_two">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_alignParentBottom="true"
                    android:layout_marginTop="14dp"
                    android:layout_marginBottom="13.5dp"
                    android:orientation="horizontal">

                    <View
                        android:id="@+id/view_bg"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="@drawable/guanli_list_bottom"></View>

                    <View
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:visibility="invisible"></View>

                    <View
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:visibility="invisible"></View>


                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <TextView
                        android:id="@+id/tv_quanbu"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center"

                        android:text="全部客户"
                        android:textColor="@color/colorWhite"
                        android:textSize="16sp"
                        android:textStyle="bold"></TextView>

                    <FrameLayout
                        android:layout_width="0dp"

                        android:layout_height="match_parent"
                        android:layout_weight="1">

                        <TextView

                            android:id="@+id/tv_daoqi"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_centerHorizontal="true"
                            android:layout_centerVertical="true"
                            android:gravity="center"
                            android:text="到期跟进"
                            android:textColor="@color/ff215dff"
                            android:textSize="16sp"
                            android:textStyle="bold"></TextView>

                        <TextView
                            android:id="@+id/tv_num"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_horizontal"
                            android:layout_marginLeft="33dp"
                            android:layout_marginTop="15dp"
                            android:background="@mipmap/hd"
                            android:gravity="center"
                            android:text="0"
                            android:textColor="@color/colorWhite"
                            android:textSize="8sp"></TextView>


                    </FrameLayout>


                    <TextView
                        android:id="@+id/tv_chengjiao"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="已成交"
                        android:textColor="@color/ff215dff"
                        android:textSize="16sp"
                        android:textStyle="bold"></TextView>

                </LinearLayout>


            </RelativeLayout>

            <View
                android:id="@+id/viewbg"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/text_1b1b1b"
                android:visibility="gone"></View>
        </RelativeLayout>

        <View
            android:id="@+id/viewbgall"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/text_1b1b1b"
            android:visibility="gone"></View>
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/rl_left"
        android:layout_gravity="left"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout

            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/colorWhite"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="24dp"
                android:layout_marginTop="70dp"
                android:orientation="horizontal">

                <com.yhkhgl.top.utils.sunUi.circularImage.CircularImage
                    android:id="@+id/iv_avatar"
                    android:layout_width="56dp"
                    android:layout_height="56dp"
                    android:src="@mipmap/img_one"></com.yhkhgl.top.utils.sunUi.circularImage.CircularImage>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="20dp"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="tv_name"
                        android:textColor="@color/text_1b1b1b"
                        android:textSize="15sp"
                        android:textStyle="bold"></TextView>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="bottom"
                        android:text="tv_name"
                        android:textColor="#FF9A9A9A"
                        android:textSize="11sp"
                        android:textStyle="bold"></TextView>
                </LinearLayout>
            </LinearLayout>

            <RelativeLayout
                android:id="@+id/info_rl"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="50dp"
                android:paddingLeft="14dp"
                android:paddingTop="10dp"
                android:paddingRight="14dp"
                android:paddingBottom="10dp">

                <ImageView
                    android:id="@+id/iv_yijian"
                    android:layout_width="15dp"
                    android:layout_height="15dp"
                    android:src="@mipmap/setup_info_icon">

                </ImageView>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="8.5dp"
                    android:layout_toRightOf="@+id/iv_yijian"
                    android:text="基本信息"
                    android:textColor="@color/text_1b1b1b"
                    android:textSize="12sp"></TextView>

                <ImageView
                    android:layout_width="13dp"
                    android:layout_height="13dp"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:src="@mipmap/gengduo"></ImageView>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/zhanghu_rl"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:paddingLeft="14dp"
                android:paddingTop="10dp"
                android:paddingRight="14dp"
                android:paddingBottom="10dp">

                <ImageView
                    android:id="@+id/zhanghu_iv"
                    android:layout_width="15dp"
                    android:layout_height="15dp"
                    android:src="@mipmap/set_up_gl_icon">

                </ImageView>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="8.5dp"
                    android:layout_toRightOf="@+id/zhanghu_iv"
                    android:text="账户管理"
                    android:textColor="@color/text_1b1b1b"
                    android:textSize="12sp"></TextView>

                <ImageView
                    android:layout_width="13dp"
                    android:layout_height="13dp"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:src="@mipmap/gengduo"></ImageView>
            </RelativeLayout>

            <RelativeLayout
                android:visibility="gone"
                android:id="@+id/fankui_rl"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:paddingLeft="14dp"
                android:paddingTop="10dp"
                android:paddingRight="14dp"
                android:paddingBottom="10dp">

                <ImageView
                    android:id="@+id/iv_fankui"
                    android:layout_width="15dp"
                    android:layout_height="15dp"
                    android:src="@mipmap/setup_icon_fankui">

                </ImageView>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="8.5dp"
                    android:layout_toRightOf="@+id/iv_fankui"
                    android:text="问题反馈"
                    android:textColor="@color/text_1b1b1b"
                    android:textSize="12sp"></TextView>

                <ImageView
                    android:layout_width="13dp"
                    android:layout_height="13dp"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:src="@mipmap/gengduo"></ImageView>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/lxwm_rl"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:paddingLeft="14dp"
                android:paddingTop="10dp"
                android:paddingRight="14dp"
                android:paddingBottom="10dp">

                <ImageView
                    android:id="@+id/iv_lxwm"
                    android:layout_width="15dp"
                    android:layout_height="15dp"
                    android:src="@mipmap/setup_icon_emill">

                </ImageView>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="8.5dp"
                    android:layout_toRightOf="@+id/iv_lxwm"
                    android:text="联系我们"
                    android:textColor="@color/text_1b1b1b"
                    android:textSize="12sp"></TextView>

                <ImageView
                    android:layout_width="13dp"
                    android:layout_height="13dp"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:src="@mipmap/gengduo"></ImageView>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/gywm_rl"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:paddingLeft="14dp"
                android:paddingTop="10dp"
                android:paddingRight="14dp"
                android:paddingBottom="10dp">

                <ImageView
                    android:id="@+id/iv_guanyuwomen"
                    android:layout_width="15dp"
                    android:layout_height="15dp"
                    android:src="@mipmap/setup_icon_guanyu">

                </ImageView>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="8.5dp"
                    android:layout_toRightOf="@+id/iv_guanyuwomen"
                    android:text="关于我们"
                    android:textColor="@color/text_1b1b1b"
                    android:textSize="12sp"></TextView>

                <ImageView
                    android:layout_width="13dp"
                    android:layout_height="13dp"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:src="@mipmap/gengduo"></ImageView>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/clear_rl"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:paddingLeft="14dp"
                android:paddingTop="10dp"
                android:paddingRight="14dp"
                android:paddingBottom="10dp">

                <ImageView
                    android:id="@+id/iv_qingchu"
                    android:layout_width="15dp"
                    android:layout_height="15dp"
                    android:src="@mipmap/icon_qingcu">

                </ImageView>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="8.5dp"
                    android:layout_toRightOf="@+id/iv_qingchu"
                    android:text="清除缓存"
                    android:textColor="@color/text_1b1b1b"
                    android:textSize="12sp"></TextView>

                <ImageView
                    android:layout_width="13dp"
                    android:layout_height="13dp"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:src="@mipmap/gengduo"></ImageView>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/out_rl"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:paddingLeft="14dp"
                android:paddingTop="10dp"
                android:paddingRight="14dp"
                android:paddingBottom="10dp">

                <ImageView
                    android:id="@+id/iv_out"
                    android:layout_width="15dp"
                    android:layout_height="15dp"
                    android:src="@mipmap/setup_out_icon">

                </ImageView>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="8.5dp"
                    android:layout_toRightOf="@+id/iv_out"
                    android:text="退出登录"
                    android:textColor="@color/text_1b1b1b"
                    android:textSize="12sp"></TextView>

                <ImageView
                    android:layout_width="13dp"
                    android:layout_height="13dp"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:src="@mipmap/gengduo"></ImageView>
            </RelativeLayout>
        </LinearLayout>
        <RelativeLayout
            android:layout_marginRight="5dp"
            android:layout_marginLeft="5dp"
            android:layout_marginBottom="30dp"
            android:layout_alignParentBottom="true"
            android:background="@mipmap/bg_img_one"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:layout_centerVertical="true"
                android:orientation="vertical"
                android:paddingLeft="34dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <ImageView

                    android:src="@mipmap/icon_diannao"
                    android:layout_width="13dp"
                    android:layout_height="13dp"></ImageView>
                <TextView
                    android:layout_marginTop="12dp"
                    android:textSize="18sp"
                    android:textStyle="bold"
                    android:textColor="@color/text_1b1b1b"
                    android:text="连接电脑"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"></TextView>
                <TextView
                    android:textSize="10sp"
                    android:layout_marginTop="10dp"
                    android:text="注：请在WiFi下使用"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"></TextView>
            </LinearLayout>
            <LinearLayout
                android:gravity="center"
                android:orientation="vertical"
                android:layout_marginRight="20dp"
                android:id="@+id/ll_lianjie"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:background="@drawable/donghua_true"
                android:layout_width="100dp"
                android:layout_height="100dp">
                <TextView
                    android:textStyle="bold"
                    android:id="@+id/tv_lianjie"
                    android:textSize="12sp"
                    android:textColor="#2a70fd"
                    android:gravity="center"
                    android:text="点击\n连接"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"></TextView>
            </LinearLayout>
        </RelativeLayout>
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="right"
        android:background="@color/colorWhite"
        android:orientation="vertical">

        <RelativeLayout
            android:id="@+id/saixuan_title"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="8dp">

            <ImageView
                android:layout_width="32dp"
                android:layout_height="35dp"
                android:padding="8dp"
                android:src="@mipmap/fanhui"
                android:visibility="invisible"></ImageView>
        </RelativeLayout>

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/saixuan_title"
            android:layout_marginBottom="60dp">

            <LinearLayout

                android:id="@+id/ll_saixuan"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/colorWhite"
                android:orientation="vertical">

            </LinearLayout>
        </ScrollView>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:layout_alignParentBottom="true"
            android:background="@color/colorWhite"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center">

                <Button
                    android:id="@+id/btn_clear"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="15dp"
                    android:background="@drawable/btn_bg"
                    android:text="重置"
                    android:textColor="#FF215DFF"
                    android:textSize="14sp"></Button>
            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center">

                <Button
                    android:id="@+id/btn_true"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="15dp"
                    android:background="@drawable/btn_bg_two"
                    android:text="确认"
                    android:textColor="@color/colorWhite"
                    android:textSize="14sp"></Button>

            </LinearLayout>
        </LinearLayout>
    </RelativeLayout>
</androidx.drawerlayout.widget.DrawerLayout>
