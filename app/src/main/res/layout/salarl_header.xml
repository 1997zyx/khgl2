<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <RelativeLayout
        android:orientation="vertical"
        android:paddingLeft="20dp"
        android:paddingTop="90dp"
        android:background="@mipmap/yongjin_bg_onexq"
        android:layout_width="match_parent"
        android:layout_height="364dp">

        <TextView
            android:id="@+id/tv_money_name"
            android:textColor="@color/colorWhite"
            android:textSize="13sp"
            android:text="当月实际工资"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"></TextView>
        <TextView
            android:id="@+id/tv_total"
            android:layout_marginTop="22dp"
            android:textColor="@color/colorWhite"
            android:textSize="45sp"
            android:text=""
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"></TextView>
        <LinearLayout
            android:layout_marginBottom="100dp"
            android:id="@+id/ll_title"
            android:layout_alignParentBottom="true"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout

                android:gravity="center"
                android:orientation="vertical"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="wrap_content">
                <TextView
                    android:id="@+id/monthly_salary_tv"
                    android:textStyle="bold"
                    android:textColor="@color/colorWhite"
                    android:textSize="14sp"
                    android:text=""
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"></TextView>
                <TextView
                    android:layout_marginTop="10dp"
                    android:textColor="#FF98B3FD"
                    android:textSize="10sp"
                    android:text="当月薪资总计"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"></TextView>
            </LinearLayout>
            <LinearLayout
                android:gravity="center"
                android:orientation="vertical"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="wrap_content">
                <TextView
                    android:id="@+id/sb_tv"
                    android:textStyle="bold"
                    android:textColor="@color/colorWhite"
                    android:textSize="14sp"
                    android:text=""
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"></TextView>
                <TextView
                    android:layout_marginTop="10dp"

                    android:textColor="#FF98B3FD"
                    android:textSize="10sp"
                    android:text="当月社保/迟到"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"></TextView>
            </LinearLayout>
            <LinearLayout
                android:gravity="center"
                android:orientation="vertical"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="wrap_content">
                <TextView
                    android:id="@+id/tv_qita"
                    android:textStyle="bold"
                    android:textColor="@color/colorWhite"
                    android:textSize="14sp"
                    android:text=""
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"></TextView>
                <TextView
                    android:layout_marginTop="10dp"
                    android:textColor="#FF98B3FD"
                    android:textSize="10sp"
                    android:text="其他"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"></TextView>
            </LinearLayout>
        </LinearLayout>
    </RelativeLayout>

    <LinearLayout
        android:id="@+id/ll_content"
        android:orientation="vertical"
        android:paddingBottom="37dp"
        android:paddingRight="24dp"
        android:paddingLeft="24dp"
        android:paddingTop="26dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="14dp"
        android:layout_marginTop="280dp"
        android:layout_marginRight="14dp"
        android:background="@drawable/bg_yuanjiao_10dp">
        <LinearLayout

            android:gravity="center_vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <View
                android:background="@drawable/salary_view_bg"
                android:layout_width="4.5dp"
                android:layout_height="15.5dp"></View>
            <TextView

                android:textStyle="bold"
                android:layout_marginLeft="7dp"
                android:textColor="#FF000000"
                android:textSize="17sp"
                android:text="佣金明细"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"></TextView>
        </LinearLayout>
        <RelativeLayout
            android:layout_marginTop="25dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:textColor="#FF606060"
                android:textSize="14sp"
                android:text="姓\u3000\u3000名"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"></TextView>
            <TextView
                android:id="@+id/tv_name"
                android:layout_alignParentRight="true"
                android:textColor="#FF0D0D0D"
                android:textSize="14sp"
                android:text=""
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"></TextView>
        </RelativeLayout>
        <RelativeLayout
            android:layout_marginTop="25dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:textColor="#FF606060"
                android:textSize="14sp"
                android:text="平台账号"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"></TextView>
            <TextView
                android:id="@+id/tv_mobile"
                android:layout_alignParentRight="true"
                android:textColor="#FF0D0D0D"
                android:textSize="14sp"
                android:text=""
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"></TextView>
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/Rl_ticheng"
            android:layout_marginTop="25dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:textColor="#FF606060"
                android:textSize="14sp"
                android:text="提成方案"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"></TextView>
            <TextView

                android:layout_centerVertical="true"
                android:layout_marginLeft="4.5dp"
                android:textColor="#FF3268FB"
                android:textSize="10sp"
                android:gravity="center"
                android:id="@+id/wenhao"
                android:layout_alignParentRight="true"
                android:background="@drawable/wenhao_bg"
                android:text=" ? "
                android:layout_width="15dp"
                android:layout_height="15dp"></TextView>
            <TextView
                android:id="@+id/fangan_tv"
                android:layout_toLeftOf="@+id/wenhao"

                android:textColor="#FF0D0D0D"
                android:textSize="14sp"
                android:text="王倩倩"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"></TextView>
        </RelativeLayout>
        <RelativeLayout
            android:layout_marginTop="25dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:textColor="#FF606060"
                android:textSize="14sp"
                android:text="底\u3000\u3000薪"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"></TextView>
            <TextView
                android:id="@+id/tv_dx"
                android:layout_alignParentRight="true"
                android:textColor="#FF0D0D0D"
                android:textSize="14sp"
                android:text=""
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"></TextView>
        </RelativeLayout>
        <RelativeLayout
            android:layout_marginTop="25dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:textColor="#FF606060"
                android:textSize="14sp"
                android:text="社\u3000\u3000保"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"></TextView>

            <TextView
                android:id="@+id/tv_sb"
                android:layout_alignParentRight="true"
                android:textColor="#FF0D0D0D"
                android:textSize="14sp"
                android:text=""
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"></TextView>
        </RelativeLayout>
        <RelativeLayout
            android:layout_marginTop="25dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:textColor="#FF606060"
                android:textSize="14sp"
                android:text="当月奖金"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"></TextView>
            <TextView
                android:id="@+id/tv_jiangjin"
                android:layout_alignParentRight="true"
                android:textColor="#FF0D0D0D"
                android:textSize="14sp"
                android:text=""
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"></TextView>
        </RelativeLayout>
        <RelativeLayout
            android:layout_marginTop="25dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:textColor="#FF606060"
                android:textSize="14sp"
                android:text="提成金额"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"></TextView>
            <TextView
                android:id="@+id/tc_tv"
                android:layout_alignParentRight="true"
                android:textColor="#FF0D0D0D"
                android:textSize="14sp"
                android:text=""
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"></TextView>
        </RelativeLayout>
        <RelativeLayout
            android:layout_marginTop="25dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:textColor="#FF606060"
                android:textSize="14sp"
                android:text="迟到次数"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"></TextView>
            <TextView
                android:id="@+id/tv_chidao"
                android:layout_alignParentRight="true"
                android:textColor="#FF0D0D0D"
                android:textSize="14sp"
                android:text=""
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"></TextView>
        </RelativeLayout>
        <RelativeLayout
            android:layout_marginTop="25dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:textColor="#FF606060"
                android:textSize="14sp"
                android:text="迟到金额"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"></TextView>
            <TextView
                android:id="@+id/tv_cdje"
                android:layout_alignParentRight="true"
                android:textColor="#FF0D0D0D"
                android:textSize="14sp"
                android:text=""
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"></TextView>
        </RelativeLayout>
        <RelativeLayout
            android:layout_marginTop="25dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:textColor="#FF606060"
                android:textSize="14sp"
                android:text="其\u3000\u3000他"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"></TextView>
            <TextView

                android:id="@+id/tv_qt"
                android:layout_alignParentRight="true"
                android:textColor="#FF0D0D0D"
                android:textSize="14sp"
                android:text=""
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"></TextView>
        </RelativeLayout>
        <RelativeLayout
            android:layout_marginTop="25dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:textColor="#FF606060"
                android:textSize="14sp"
                android:text="成  交  额"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"></TextView>
            <TextView
                android:id="@+id/tv_cje"
                android:layout_alignParentRight="true"
                android:textColor="#FF0D0D0D"
                android:textSize="14sp"
                android:text=""
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"></TextView>
        </RelativeLayout>
        <RelativeLayout
            android:layout_marginTop="25dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:textColor="#FF606060"
                android:textSize="14sp"
                android:text="当月佣金"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"></TextView>
            <TextView
                android:id="@+id/tv_dyyj"
                android:layout_alignParentRight="true"
                android:textColor="#FF0D0D0D"
                android:textSize="14sp"
                android:text=""
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"></TextView>
        </RelativeLayout>


    </LinearLayout>
    <LinearLayout
        android:layout_marginTop="24dp"
        android:layout_marginBottom="15dp"
        android:layout_marginRight="14dp"
        android:layout_marginLeft="14dp"
        android:id="@+id/ll_title_two"
        android:layout_below="@+id/ll_content"
        android:gravity="center_vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <View
            android:background="@drawable/salary_view_bg"
            android:layout_width="4.5dp"
            android:layout_height="15.5dp"></View>
        <TextView
            android:textStyle="bold"
            android:layout_marginLeft="7dp"
            android:textColor="#FF000000"
            android:textSize="17sp"
            android:text="薪资明细"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"></TextView>
    </LinearLayout>
    <LinearLayout
        android:layout_below="@+id/ll_title_two"
        android:orientation="vertical"
        android:id="@+id/ll_zhanwu"
        android:visibility="gone"
        android:gravity="center"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <ImageView
            android:layout_gravity="center"
            android:layout_width="143dp"
            android:layout_height="66dp"
            android:layout_marginTop="10dp"
            android:src="@mipmap/product__img_zanwu">
        </ImageView>
        <TextView
            android:layout_margin="15dp"
            android:gravity="center"
            android:text="暂无数据"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"></TextView>
    </LinearLayout>
</RelativeLayout>